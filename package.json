{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "https://YungSeepferd.github.io/Portfolio",
  "dependencies": {
    "@emailjs/browser": "^4.3.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.12",
    "@react-three/drei": "^9.99.5",
    "@react-three/fiber": "^8.15.16",
    "framer-motion": "^11.0.8",
    "pdfjs-dist": "^4.4.168",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^7.7.1",
    "react-router-dom": "^6.22.2",
    "react-scroll": "^1.9.0",
    "react-swipeable": "^7.0.1",
    "styled-components": "^6.1.8",
    "three": "^0.162.0",
    "tone": "^15.1.22",
    "zod": "^4.0.15",
    "zustand": "^4.5.2"
  },
  "overrides": {
    "react-pdf": {
      "pdfjs-dist": ">=4.2.0"
    }
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test e2e/visual.spec.ts --update-snapshots",
    "test:visual:verify": "playwright test e2e/visual.spec.ts",
    "test:perf": "vitest --config vitest.perf.config.ts",
    "test:mutation": "stryker run",
    "lint": "eslint --config eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\" --report-unused-disable-directives",
    "lint:fix": "eslint --config eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --config eslint.config.js --fix",
      "prettier --write"
    ],
    "src/**/*.{json,md,yml,css,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@emotion/is-prop-valid": "^1.3.1",
    "@playwright/test": "^1.54.2",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/vitest-runner": "^9.0.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "sharp": "^0.34.3",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-plugin-image-optimizer": "^2.0.2",
    "vitest": "^3.2.4"
  }
}
